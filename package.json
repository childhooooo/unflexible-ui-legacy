  "scripts": {
    "dev": "run-p storybook build:watch",
    "storybook": "start-storybook -p 56006",
    "build-storybook": "build-storybook",
    "build": "run-p build:esbuild build:types",
    "build:watch": "WATCH=true npm run build:esbuild",
    "build:esbuild": "node --require esbuild-register build.ts",
    "build:types": "tsc --declaration --emitDeclarationOnly --declarationDir './build'",
    "prepare": "npm run build",
    "fix": "run-s fix:prettier fix:eslint fix:stylelint",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:prettier": "npm run lint:prettier -- --write",
    "fix:stylelint": "npm run lint:stylelint -- --fix",
    "lint": "run-p -l -c --aggregate-output lint:*",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "lint:stylelint": "stylelint './src/**/*.{jsx,tsx}'",
    "stylelint": "stylelint"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "extends": [
      "react-app",
      "plugin:@typescript-eslint/eslint-recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier"
    ],
    "rules": {
      "no-use-before-define": "off",
      "@typescript-eslint/no-use-before-define": [
        "error"
      ]
    }
  },
  "browserslist": [
    "> 0.25% in JP",
    "last 2 versions",
    "Firefox ESR",
    "not dead",
    "ie >= 11"
